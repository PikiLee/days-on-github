{
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20.16.0"
  },
  "scripts": {
    "prepare": "nitropack prepare",
    "dev": "run-p build:css:watch build:render:watch dev:nitro",
    "build": "run-s build:app build:nitro",
    "dev:nitro": "nitropack dev",
    "build:nitro": "nitropack build",
    "preview": "node .output/server/index.mjs",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:once": "vitest run",
    "dev:app": "vite",
    "build:app": "pnpm run build:css && pnpm run build:render",
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/output.css --minify",
    "build:render": "vite build --outDir dist/server --ssr src/render.tsx",
    "build:css:watch": "tailwindcss -i ./src/index.css -o ./dist/output.css --minify --watch",
    "build:render:watch": "vite build --outDir dist/server --ssr src/render.tsx --watch"
  },
  "dependencies": {
    "@sparticuz/chromium-min": "126",
    "@vercel/blob": "^0.23.4",
    "clsx": "^2.1.1",
    "nitropack": "^2.9.7",
    "puppeteer-core": "22.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "winston": "^3.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@eslint-react/eslint-plugin": "^1.7.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.0.1",
    "jsdom": "^24.1.1",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "rollup-plugin-import-contents": "^1.0.4",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  }
}

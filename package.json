{
  "private": true,
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
  "scripts": {
    "prepare": "nitropack prepare",
    "dev": "run-p build:css:watch build:render:watch dev:nitro",
    "build": "run-s build:app build:nitro",
    "dev:nitro": "nitropack dev",
    "build:nitro": "nitropack build",
    "preview": "node .output/server/index.mjs",
    "lint": "eslint . --fix",
    "test": "vitest",
    "dev:app": "vite",
    "build:app": "pnpm run build:css && pnpm run build:render",
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/output.css --minify",
    "build:render": "vite build --outDir dist/server --ssr src/render.tsx",
    "build:css:watch": "tailwindcss -i ./src/index.css -o ./dist/output.css --minify --watch",
    "build:render:watch": "vite build --outDir dist/server --ssr src/render.tsx --watch",
    "package": "esno package.mts"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "nitropack": "latest",
    "node-html-to-image": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/adm-zip": "^0.5.5",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "adm-zip": "^0.5.14",
    "autoprefixer": "^10.4.19",
    "enquirer": "^2.4.1",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "esno": "^4.7.0",
    "nitro-test-utils": "^0.5.0",
    "npm-run-all2": "^6.2.2",
    "picocolors": "^1.0.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20"
  }
}
